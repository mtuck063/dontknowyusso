<svg class="hamburger" height="40" width="50">
  <line x1="10" y1="10" x2="40" y2="10" />
  <line x1="10" y1="20" x2="40" y2="20" />
  <line x1="10" y1="30" x2="40" y2="30" />
</svg>

<svg class="close-x" height="50" width="50">
  <line x1="10" y1="10" x2="40" y2="40" />
  <line x1="10" y1="40" x2="40" y2="10" />
</svg>

<div id="header-container">

  <div class="row">
    <div class="col s12 m12 l6" data-no-turbolink>
          <%= link_to(root_path, class: 'header-link')  do %>
        <%= image_tag("land_page.jpg", class: 'icon') %>
        <span class="link-caption" data-menu="HOME"></span>
      <%end%>
    </div>
    <div class="col s12 m12 l6" data-no-turbolink>
      <%= link_to(artist_path, class: 'header-link')  do %>
        <%= image_tag("bio_page.jpg", class: 'icon') %>
        <span class="link-caption" data-menu="Y&#217;SS&#211;"></span>
      <%end%>
    </div>
    <div class="col s12 m12 l6" data-no-turbolink>
      <%= link_to(art_path, class: 'header-link')  do %>
        <%= image_tag("music_page.jpg", class: 'icon') %>
        <span class="link-caption" data-menu="MUSIC"></span>
      <%end%>
    </div>
    <div class="col s12 m12 l6" data-no-turbolink>
      <%= link_to(shows_path, class: 'header-link')  do %>
        <%= image_tag("show_page.jpg", class: 'icon') %>
        <span class="link-caption" data-menu="SHOWS"></span>
      <%end%>
    </div>
  </div>
	
</div>
